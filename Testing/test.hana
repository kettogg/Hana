fn max(int a, int b) : int
  if a > b
    return a
  else
    return b

fn min(int a, int b) : int
  if a > b
    return b
  return a

fn testif(int a, int b) : int
    int x = 0
    if a == b
        return a
    else
        x = b
    return x

fn testifMax(int a, int b) : int
    int max = 0
    if a > b
        max = a
    else
        max = b
    return max

int minmax = max( 1,10)
write("Max is %d expected 10\n", minmax)
minmax = min( 1,10)
write("Min is %d\ expected 1n", minmax)
int nif = testif( 1,10)
write("testif is %d expected 10\n", nif)

nif = testif( 5,5)
write("testif is %d expected 5\n", nif)

write("testifMax is %d expected 10 \n", testifMax(1,10))
write("testifMax is %d expected 10 \n", testifMax(10,1))